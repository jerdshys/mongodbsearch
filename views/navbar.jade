              a(href="#") One more separated link
     
      ul.nav.navbar-nav.navbar-right
        if(!user)
          li
            a(href="/register") register
          li.active
            a(href="/login") login
        if(user)
          li
            a(href="/profile") My account : #{user.username}
          li
            a(href="/logout") Logout
      
       