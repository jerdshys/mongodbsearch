          span(class="icon-bar")
          span(class="icon-bar")
          span(class="icon-bar")
        a(class="navbar-brand" href="#") 
          i.fa.fa-home
    .navbar-collapse.collapse
      ul.nav.navbar-nav
        li.active
          a(href="#") Catalogue
        li
          a(href="#") Producteurs
